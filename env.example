# Application Configuration
NODE_ENV=development
PORT=8000
APP_NAME="Real Estate Photo AI Backend"
APP_VERSION=1.0.0

# API Configuration
API_PREFIX=/api/v1
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# Replicate API Configuration
# ‚ö†Ô∏è  ADD YOUR REAL TOKEN HERE:
REPLICATE_API_TOKEN=your_replicate_api_token_here

# üöÄ JUGGERNAUT XL v9 RUNDIFFUSION - PHOTOREALISTIC RESULTS!
# Based on successful Discord posts showing depth + inpainting workflow
STABLE_DIFFUSION_MODEL=lucataco/juggernaut-xl-v9:latest

# Alternative Juggernaut models:
# STABLE_DIFFUSION_MODEL=lucataco/juggernaut-xl-v8:latest
# STABLE_DIFFUSION_MODEL=playgroundai/playground-v2.5-1024px-aesthetic:latest

# JUGGERNAUT XL + DEPTH WORKFLOW SETTINGS
DEFAULT_PROMPT=modern furnished living room, stylish furniture, warm lighting, professional interior design, photorealistic
NEGATIVE_PROMPT=blurry, low quality, distorted, cluttered, messy, dark, poor lighting, oversaturated, unrealistic

# Depth + Inpainting Workflow (Two-Pass Process)
USE_CONTROLNET=true
CONTROLNET_MODEL=depth
CONTROLNET_STRENGTH=0.8
STRUCTURE_PRESERVATION_STRENGTH=0.65
DEFAULT_TRANSFORMATION_STRENGTH=0.65
ENABLE_INPAINTING_WORKFLOW=true

# Security
API_KEY=your_optional_api_key_here
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Logging
LOG_LEVEL=info
LOG_FORMAT=combined

# File Storage
UPLOAD_DIR=uploads
OUTPUT_DIR=outputs
TEMP_DIR=temp

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100 